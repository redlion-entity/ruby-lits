<%= form_for([@twit, @twit.comments.build]) do |f| %>
    <p>
      <%= f.label :user_id %><br>
      <%= f.collection_select :user_id, User.all, :id, :nickname, prompt: true %>
    </p>
    <p>
      <%= f.label :body %><br>
      <%= f.text_area :body %>
    </p>
    <p>
      <%= f.submit %>
    </p>
<% end %>